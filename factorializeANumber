// Return the factorial of the provided integer. Factorials are ofter represented with the shorthand notation n! 
//for ex: 5! = 1 * 2 * 3 * 4 * 5 = 120

function factorialize(num) {
  if(num === 0) {
    return 1;
    }
} return num * factorialize (num - 1);
}

//if statement is the terminal condition to end the recursion
